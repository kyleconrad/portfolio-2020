{
	"name": "portfolio-2020",
	"private": true,
	"description": "Gatsby build of Kyle Conrad's 2020 portfolio redesign/rebuild using Contentful as the content management system.",
	"version": "1.1.0",
	"license": "MIT",
	"scripts": {
		"clean": "gatsby clean",
		"format": "prettier --write \"**/*.{js,jsx,json,md}\"",
		"develop": "gatsby develop -o",
		"start": "npm run develop",
		"build": "gatsby build",
		"serve": "gatsby serve -o",
		"deploy": "gatsby-plugin-s3 deploy --yes && aws cloudfront create-invalidation --distribution-id E3RLBNZCU23MII --paths \"/*\""
	},
	"dependencies": {
		"@contentful/rich-text-react-renderer": "^13.4.0",
		"@contentful/rich-text-types": "^14.1.0",
		"classnames": "^2.2.6",
		"gatsby": "^2.20.3",
		"gatsby-image": "^2.3.1",
		"gatsby-plugin-google-gtag": "^3.1.0",
		"gatsby-plugin-html-comments": "^1.0.0",
		"gatsby-plugin-react-helmet": "^3.2.1",
		"gatsby-plugin-s3": "^0.3.3",
		"gatsby-plugin-sass": "^2.2.1",
		"gatsby-plugin-sharp": "^2.5.2",
		"gatsby-plugin-sitemap": "^2.3.1",
		"gatsby-plugin-zopfli": "^1.3.2",
		"gatsby-source-contentful": "^2.2.9",
		"gatsby-transformer-remote-filesystem": "^1.0.0",
		"gatsby-transformer-sharp": "^2.4.2",
		"node-sass": "^4.13.1",
		"react": "^16.13.1",
		"react-dom": "^16.13.1",
		"react-ga": "^2.7.0",
		"react-helmet": "^5.2.1"
	},
	"devDependencies": {
		"prettier": "^2.0.1"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/kyleconrad/portfolio-2020"
	}
}
